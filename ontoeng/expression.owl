    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasOperand -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasOperand">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#UnaryExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasOperands -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasOperands">
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#IteratedBinaryExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasOperator -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasOperator">
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#ComplexExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Operator"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasRhsOperand -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasRhsOperand">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#BinaryExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:ObjectProperty>
    


