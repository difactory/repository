    <!-- http://www.ontoeng.com/expression#hasControlFlowAction -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasControlFlowAction">
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#ControlFlowExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasControlFlowCondition -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasControlFlowCondition">
        <rdfs:domain rdf:resource="http://www.ontoeng.com/expression#ControlFlowExpression"/>
        <rdfs:range rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontoeng.com/expression#hasLhsOperand -->

    <owl:ObjectProperty rdf:about="http://www.ontoeng.com/expression#hasLhsOperand">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
