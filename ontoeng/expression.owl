    <!-- http://www.ontoeng.com/expression#Variable -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#Variable">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#AtomicExpression"/>
    </owl:Class>
    


