            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#UnaryOperator -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#UnaryOperator">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#Operator"/>
