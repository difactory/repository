    <!-- http://www.ontoeng.com/expression#AtomicExpression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#AtomicExpression">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#Expression"/>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#BinaryExpression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#BinaryExpression">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#ComplexExpression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasLhsOperand"/>
                <owl:someValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasRhsOperand"/>
                <owl:someValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasLhsOperand"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperator"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#BinaryOperator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasRhsOperand"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#BinaryOperator -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#BinaryOperator">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#Operator"/>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#CommutativeBinaryOperator -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#CommutativeBinaryOperator">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#BinaryOperator"/>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#ComplexExpression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#ComplexExpression">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperator"/>
                <owl:someValuesFrom rdf:resource="http://www.ontoeng.com/expression#Operator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperator"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Operator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#Constant -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#Constant">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#AtomicExpression"/>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#ControlFlowExpression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#ControlFlowExpression">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#ComplexExpression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasControlFlowAction"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasControlFlowCondition"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperator"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#ControlFlowStatement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#ControlFlowStatement -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#ControlFlowStatement">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#Operator"/>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#Expression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#Expression"/>
    


    <!-- http://www.ontoeng.com/expression#IteratedBinaryExpression -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#IteratedBinaryExpression">
        <rdfs:subClassOf rdf:resource="http://www.ontoeng.com/expression#ComplexExpression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperands"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperator"/>
                <owl:allValuesFrom rdf:resource="http://www.ontoeng.com/expression#CommutativeBinaryOperator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontoeng.com/expression#hasOperands"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ontoeng.com/expression#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ontoeng.com/expression#Operator -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#Operator"/>
    


    <!-- http://www.ontoeng.com/expression#RandomVariable -->

    <owl:Class rdf:about="http://www.ontoeng.com/expression#RandomVariable">
